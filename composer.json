{
    "name": "mithatguner/googlecaptcha",
    "description": "No Captcha reCaptcha For Laravel",
    "require": {
        "php": ">=5.5.5",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "mithatguner\\googlecaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mithatguner\\googlecaptcha\\GoogleCaptchaServiceProvider"
            ],
            "aliases": {
                "googlecaptcha": "mithatguner\\googlecaptcha\\Facades\\GoogleCaptcha"
            }
        }
    },
    "license": "MIT"
}
